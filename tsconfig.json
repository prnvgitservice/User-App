{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": false,
    "allowImportingTsExtensions": true,
    "skipLibCheck": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    // "**/*.jsx",
    "**/*.d.ts",
    "nativewind-env.d.ts"
  ]
}
